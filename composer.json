{
    "name": "pugx/shortid-php",
    "description": "An implementation of shortid in PHP",
    "keywords": ["shortid", "id", "uuid"],
    "homepage": "http://pugx.org/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        },
        {
            "name": "The Community",
            "homepage": "https://github.com/pugx/shortid-php"
        }
    ],
    "support": {
        "issues": "https://github.com/pugx/shortid-php/issues"
    },
    "require": {
        "php": "^7.0",
        "paragonie/random-lib": "^2.0",
        "symfony/polyfill-mbstring": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2 || ^7.0"
    },
    "autoload": {
        "psr-4": { "PUGX\\Shortid\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "PUGX\\Shortid\\Test\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
